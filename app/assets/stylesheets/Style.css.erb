@font-face {
  font-family: 'SplitCustomGotham';
  src:font-url('Gotham-ExtraLight.eot');
  src:font-url('Gotham-ExtraLight.eot?#iefix') format('embedded-opentype'),
    font-url('Gotham-ExtraLight.svg#icomoon') format('svg'),
    font-url('Gotham-ExtraLight.woff') format('woff'),
    font-url('Gotham-ExtraLight.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
}

.bgImage { 
	width:100%;
	height:100%;
  	background: image-url('logoLarge.png') no-repeat center center fixed; 
  	-webkit-background-size: cover;
  	-moz-background-size: cover;
  	-o-background-size: cover;
  	background-size: cover;
}

a, a:hover, a:visited {text-decoration:none;color:black;}

